# makecint -mk Makefile -dl pthread.dll -H pthd.h -D_POSIX_C_SOURCE=199506L -l -lpthread -cint -M0x1c
makecint -mk Makefile -o pthread -H pthd.h -D_POSIX_C_SOURCE=199506L -l -lpthread -cint -M0x1c -Z0
make
